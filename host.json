{
    "version": "2.0",
    "logging": {
        "logLevel": {
            "default": "Information"
        },
        "ApplicationInsights": {
            "LogLevel": {
                "Default": "Warning"
            }
        }
    },
    "extensions": {
        "queues": {
            "maxPollingInterval": "00:00:01",
            "visibilityTimeout": "00:00:30",
            "batchSize": 8,
            "maxDequeueCount": 5,
            "newBatchThreshold": 4
        }
    }
}